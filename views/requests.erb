<ul id="sent-requests">
  <% @sent_requests.each do |request| %>
    <h2>
      Sent request for: <a href="requests/<%=request.id%>"><%= request.space.name %></a>
    </h2>
    <li>Status: <%= request.status %></li>
  <% end %>
</ul>

<ul id="received-requests">
  <% @received_requests.each do |request| %>
    <li>Received request for: <%= request.space.name %></li>
    <li>Status: <%= request.status %></li>
  <% end %>
</ul>
